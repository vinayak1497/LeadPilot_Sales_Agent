{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "ui_client/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/css/(.*)",
      "dest": "/ui_client/static/css/$1"
    },
    {
      "src": "/static/js/(.*)",
      "dest": "/ui_client/static/js/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/ui_client/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
